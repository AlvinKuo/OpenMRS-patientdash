<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>patientdash</id>
	<name>Patient DICOM Viewer Module</name>
	<version>1.0</version>
	<package>org.openmrs.module.patientdash</package>
	<author>Kuo</author>
	<description>
		DICOM Viewer
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/patientdash/update.rdf</updateURL>
	<require_version>1.9.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.patientdash.PatientDICOMViewerActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.patientdash.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		PatientDICOMViewer.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with patientdash. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

